{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "services": [
    {
      "name": "ai-workbench-temporal",
      "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "Dockerfile"
      }
    },
    {
      "name": "temporal-proxy",
      "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "temporal-proxy/Dockerfile"
      }
    }
  ],
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}